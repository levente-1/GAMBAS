    "PATH": "/usr/local/bin:/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin",
    "PYTHON_VERSION": "3.11.11"
  },
  "inputs": {
    "api-key": {
      "base": "api-key",
      "read-only": true
    },
    "input": {
      "base": "file",
      "description": "input file (usually axial or isotropic reconstruction)",
