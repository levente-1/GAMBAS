    "which_epoch": {
      "type": "string",
      "default": "latest",
      "description": "Which epoch to load"
    },
    "stride_inplane": {
      "type": "integer",
      "default": 32,
      "description": "Stride size in 2D plane"
    },
    "stride_layer": {
      "type": "integer",
      "default": 32,
      "description": "Stride size in z direction"
    }
  },
  "custom": {
    "analysis-level": "participant",
    "flywheel": {
      "classification": {
        "components": [],
        "keywords": [
          "super-resolution",
          "volume",
          "template"
        ],
        "modality": [
          "MR"
        ],
        "type": [
          "nifti"
        ]
      },
      "suite": "UNITY Apps"
    },
    "gear-builder": {
      "category": "analysis",
      "image": "flywheel/gambas:0.0.1"
    },
    "kcl-app-binary": "app/main.sh",
    "kcl-app-modalities": [
      "anat"
    ],
    "license": {
      "dependencies": [],
      "main": {
        "name": "",
        "url": ""
      },
      "non-commercial-use-only": false
    }
  },
  "description": "Generalised-Hilbert Mamba for Super-resolution of Paediatric Ultra-Low-Field MRI",
  "environment": {
    "FLYWHEEL": "/flywheel/v0",
    "LANG": "C.UTF-8",
    "PWD": "/flywheel/v0",
